            <locator>[id="page:main-container"]</locator>
            <strategy>css selector</strategy>
        </userForm>
        <rolesGrid>
            <class>Magento\User\Test\Block\Adminhtml\User\Tab\Role\Grid</class>
            <locator>[id="permissionsUserRolesGrid"]</locator>
            <strategy>css selector</strategy>
        </rolesGrid>
